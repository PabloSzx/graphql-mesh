{
  "name": "@graphql-mesh/json-schema",
  "version": "0.16.1",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "devDependencies": {
    "@types/to-json-schema": "0.2.0",
    "@types/url-join": "4.0.0",
    "@types/qs": "6.9.6"
  },
  "dependencies": {
    "@ardatan/aggregate-error": "0.0.6",
    "@json-schema-tools/meta-schema": "1.6.1",
    "json-ptr": "2.2.0",
    "@graphql-mesh/types": "0.42.0",
    "@graphql-mesh/utils": "0.12.0",
    "@graphql-mesh/store": "0.0.0",
    "fast-json-patch": "3.0.0-1",
    "graphql-compose": "8.1.0",
    "graphql-scalars": "1.9.3",
    "to-json-schema": "0.2.5",
    "url-join": "4.0.1",
    "qs": "6.10.1",
    "ajv": "8.5.0",
    "ajv-formats": "2.1.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
